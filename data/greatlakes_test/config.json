{
    "dz": {
        "pattern": "http://doppler.cs.umass.edu/roost/img/greatlakes_test/dz0.5/%04d/%02d/%02d/%s/%s.png",
        "fields": ["year", "month", "day", "station", "scan"]
    },
    "vr": {
        "pattern": "http://doppler.cs.umass.edu/roost/img/greatlakes_test/vr0.5/%04d/%02d/%02d/%s/%s.png",
        "fields": ["year", "month", "day", "station", "scan"]
    },
    "boxes" : {
        "pattern": "data/%s/tracks_%s.txt",
        "fields": ["dataset", "batchid"]
    },
    "scans" : {
        "pattern": "data/%s/scans_%s.txt",
        "fields": ["dataset", "batchid"]
    },
    "filtering" : {
        "detections_min" : 1,
        "high_quality_detections_min": 1,
        "score_min" : -1,
        "avg_score_min" : -1
    }
}
